<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:id="@+id/Menu_ExplorePerhentian"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:id="@+id/ExplorePerhentianTxt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Explore Perhentian"
            android:textSize="20dp"
            android:textColor="#ffffff"
            android:background="#54C7FC"
            android:paddingTop="8dp"
            android:paddingBottom="8dp" />
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        <!-- START [Menu Explore Perhentian] -->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
            <!-- Explore1 - Marine Park Rules -->
                <LinearLayout
                    android:id="@+id/ExploreMarineParkRules"
                    android:background="@color/DarkBlue"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:clickable="true"
                    android:orientation="vertical"
                    android:paddingTop="@dimen/padding_MM_explore"
                    android:paddingBottom="@dimen/padding_MM_explore">
                    <ImageView
                        android:id="@+id/ImageTurtleA"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_playlist_add_check_white_48dp"
                        android:layout_centerVertical="true" />
                    <TextView
                        android:layout_below="@id/ImageTurtleA"
                        android:text="Marine Park Rules"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:textColor="#000000" />
                </LinearLayout>
            <!-- Explore2 - How to get here -->
                <LinearLayout
                    android:id="@+id/ExploreHowToGetHere"
                    android:background="@color/DarkerBlue"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:clickable="true"
                    android:orientation="vertical"
                    android:paddingTop="@dimen/padding_MM_explore"
                    android:paddingBottom="@dimen/padding_MM_explore">
                    <ImageView
                        android:id="@+id/ImageTurtleB"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_pin_drop_white_48dp"
                        android:layout_centerVertical="true" />
                    <TextView
                        android:layout_below="@id/ImageTurtleB"
                        android:text="How to Get Here"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:textColor="#000000" />
                </LinearLayout>
            <!-- Explore3 - Coral Watch -->
                <LinearLayout
                    android:id="@+id/ExploreCoralWatch"
                    android:background="@color/DarkBlue"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:clickable="true"
                    android:orientation="vertical"
                    android:paddingTop="@dimen/padding_MM_explore"
                    android:paddingBottom="@dimen/padding_MM_explore">
                    <ImageView
                        android:id="@+id/ImageTurtleC"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_remove_red_eye_white_48dp"
                        android:layout_centerVertical="true" />
                    <TextView
                        android:layout_below="@id/ImageTurtleC"
                        android:text="Coral Watch"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:textColor="#000000" />
                </LinearLayout> 
            <!-- Explore5 - Food and Drink -->
                <LinearLayout
                    android:id="@+id/ExploreFoodAndDrink"
                    android:background="@color/DarkerBlue"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:clickable="true"
                    android:orientation="vertical"
                    android:paddingTop="@dimen/padding_MM_explore"
                    android:paddingBottom="@dimen/padding_MM_explore">
                    <ImageView
                        android:id="@+id/ImageTurtleE"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_restaurant_white_48dp"
                        android:layout_centerVertical="true" />
                    <TextView
                        android:layout_below="@id/ImageTurtleE"
                        android:text="Food and Drink"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:textColor="#000000" />
                </LinearLayout>
            <!-- Explore6 - Malay Dictionary -->
                <LinearLayout
                    android:id="@+id/ExploreMalayDictionary"
                    android:background="@color/DarkBlue"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:clickable="true"
                    android:orientation="vertical"
                    android:paddingTop="@dimen/padding_MM_explore"
                    android:paddingBottom="@dimen/padding_MM_explore">
                    <ImageView
                        android:id="@+id/ImageTurtleF"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_import_contacts_white_48dp"
                        android:layout_centerVertical="true" />
                    <TextView
                        android:layout_below="@id/ImageTurtleF"
                        android:text="Malay Dictionary"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:textColor="#000000" />
                </LinearLayout>
            </LinearLayout>
        <!-- END [Menu Explore Perhentian] -->
        </ScrollView>
    </LinearLayout>
</FrameLayout>